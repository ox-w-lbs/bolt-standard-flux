{
  "name": "bolt-standard-flux",
  "version": "0.2.4",
  "description": "Opinionated meta config runner for react components",
  "scripts": {
    "lint-node": "eslint -c node_modules/electrode-bolt/config/eslint/.eslintrc-node server *.js --color",
    "lint-react": "eslint --ext .js,.jsx -c node_modules/electrode-bolt/config/eslint/.eslintrc-react client --color",
    "lint": "bolt lint-node && bolt lint-react",
    "start": "node server/index.js",
    "server": "nodemon --watch client --watch server server/index.js",
    "server-dev": "webpack-dev-server --config webpack.config.dev.js --progress --colors --port 2992",
    "server-hot": "webpack-dev-server --config webpack.config.hot.js --hot --progress --colors --port 2992 --inline",
    "sources": "http-server -p 3001 .",
    "watch": "webpack --watch",
    "prod": "bolt watch & bolt server & bolt sources",
    "dev": "bolt server-dev & WEBPACK_DEV=true bolt server",
    "hot": "bolt server-hot & WEBPACK_DEV=true bolt server"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:walmartreact/bolt-standard-flux.git"
  },
  "author": "chaseadamsio",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/walmartreact/bolt-standard-flux/issues"
  },
  "homepage": "https://github.com/walmartreact/bolt-standard-flux#readme",
  "dependencies": {
    "babel": "^5.8.21",
    "babel-core": "^5.8.22",
    "babel-eslint": "^4.0.10",
    "babel-loader": "^5.3.2",
    "chai": "^3.2.0",
    "chalk": "^1.1.1",
    "css-loader": "^0.16.0",
    "eslint": "^1.2.0",
    "eslint-config-defaults": "^4.1.0",
    "eslint-plugin-filenames": "^0.1.2",
    "eslint-plugin-react": "^3.2.3",
    "file-loader": "^0.8.4",
    "isparta-loader": "^0.2.0",
    "istanbul": "^0.3.18",
    "js-yaml": "^3.4.0",
    "karma": "^0.13.9",
    "karma-chrome-launcher": "^0.2.0",
    "karma-coverage": "^0.4.2",
    "karma-firefox-launcher": "^0.1.6",
    "karma-ie-launcher": "^0.2.0",
    "karma-mocha": "^0.2.0",
    "karma-mocha-reporter": "^1.1.1",
    "karma-phantomjs-launcher": "^0.2.0",
    "karma-phantomjs-shim": "^1.0.0",
    "karma-safari-launcher": "^0.1.1",
    "karma-sauce-launcher": "^0.2.14",
    "karma-sinon-chai": "^1.0.0",
    "karma-spec-reporter": "0.0.20",
    "karma-webpack": "^1.7.0",
    "lodash": "^3.10.1",
    "phantomjs": "^1.9.18",
    "raw-loader": "^0.5.1",
    "react": "^0.13.3",
    "react-hot-loader": "^1.2.8",
    "react-tools": "^0.13.2",
    "rimraf": "^2.4.0",
    "sinon": "^1.15.4",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.12.3",
    "stylus-loader": "^1.2.1",
    "url-loader": "^0.5.6",
    "webpack": "^1.11.0",
    "webpack-dev-server": "^1.10.1"
  }
}
